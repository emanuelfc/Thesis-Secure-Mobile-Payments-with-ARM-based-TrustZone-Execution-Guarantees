CC      = $(CROSS_COMPILE)gcc
LD      = $(CROSS_COMPILE)ld
AR      = $(CROSS_COMPILE)ar
NM      = $(CROSS_COMPILE)nm
OBJCOPY = $(CROSS_COMPILE)objcopy
OBJDUMP = $(CROSS_COMPILE)objdump
READELF = $(CROSS_COMPILE)readelf

OBJS = tds_benchmark.o tds_ta_entry.o

CFLAGS += -Wall -I../ta/include -I./include -I$(TEEC_EXPORT)/include

#Add/link other required libraries here
LDADD += -L$(TEEC_EXPORT)/lib -lteec

BINARY = tds_benchmark

.PHONY: all
all: $(BINARY)

$(BINARY): $(OBJS)
	$(CC) -o $@ tds_ta_entry.o $< $(LDADD)

.PHONY: clean
clean:
	rm -f $(OBJS) $(BINARY)

%.o: %.c
	$(CC) $(CFLAGS) -c $< -o $@
